package pack01_Valiable;

public class Ex03_Valiable {
	public static void main(String[] args) {
//		int value = ;
//		int result = value + 10;
		int value = 30;
		int result = value + 10;
		System.out.println(result);
		//숫자형 변수들은 사칙연산이 가능하다.
		//재할당 (변수를 계속 만드는 게 아니라 값을 다시 줘서 바꿈)
		result = 1564*478575+(2345/54);
		System.out.println(result);
	}

}
