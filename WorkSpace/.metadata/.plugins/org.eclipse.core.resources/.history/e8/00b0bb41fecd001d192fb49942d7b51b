package pack05_String;

public class Ex01_String {
	public static void main(String[] args) {
		//Java에서 제일 많이 사용되는 데이터타입
		//int(정수), double(실수), String(문자) < 대문자 주의
		// " == 쌍따옴표 == 더블 코테이션
		// 데이터를 할당할 때는 더블코테이션으로 내용을 감싸주고 할당.
		String str = "asdf" ; //초기화
		System.out.println(str);
		System.out.println(); //println()<사이에는 하나의 변수만
		//넣을 수 있음 또는 변수에 담을 수 있는 값
		String stri = "t";
		System.out.println(str=stri);
	}

}
