package pack02_array;

public class Ex02_Array {
public static void main(String[] args) {
	int[] scoreArray = new int[10];
	for(int i=0;i<10;i++) {
		scoreArray[i]=10;
		
		System.out.println("scoreArray["+i+"] : "+scoreArray[i]);
		scoreArray[i]+=10;
	}
	int i1=0;
	int j1=10;
	while(i1<10) {
		scoreArray[i1]=j1;
		System.out.println("scoreArray["+i1+"] : "+scoreArray[i1]);
		i1++;
		j1+=10;
	}
	
	
	
	
	
	
	
}
}
