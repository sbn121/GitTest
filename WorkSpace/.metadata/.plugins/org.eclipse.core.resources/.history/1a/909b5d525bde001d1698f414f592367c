package pack03_oop1;

//Student Class : 학생의 성적정보를 관리할 수 있게 만든 설계도
public class Student {
	//멤버(필드) : 학생의 번호, 이름, 국어성적, 수학성적, 영어성적, 총점, 평균
	
	int num;
	String name;
	int korSco;
	int matSco;
	int engSco;
	int sum = (korSco+matSco+engSco);
	double avg = (double)sum/3;
	void student() {
		System.out.println(num+name+korSco+matSco+engSco+sum+avg);
	}
	
}
