package pack09_scanner;

import java.util.Scanner;

public class Ex02_Scanner {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);///<<암기
		String data1 = sc.nextLine();
		/////<<암기, 내가 엔터키 치기 전까지의 문자열을 가져옴
		String data2 = sc.nextLine();
		System.out.println(data1);
		System.out.println(data2);
		//1. int sum1 = Integer.parseInt(data1);
		//1. int sum2 = Integer.parseInt(data2);
		//2. int sum = Integer.parseInt(data1)+
				  //Integer.parseInt(data2);
		//System.out.println(sum);
		System.out.println(Integer.parseInt(data1)+
				           Integer.parseInt(data2));
		
		
		
	}

}
