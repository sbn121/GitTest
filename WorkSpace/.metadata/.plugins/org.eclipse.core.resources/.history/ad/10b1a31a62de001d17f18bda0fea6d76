package pack03_oop1;

public class StudentDTO {
	int num, korSco, matSco, engSco, sum;
	String name;
	double avg;
	public StudentDTO(int num, String name, int korSco, int matSco, int engSco) {
		this.num = num;
		this.korSco = korSco;
		this.matSco = matSco;
		this.engSco = engSco;
		this.name = name;
		this.sum = korSco+matSco+engSco;
		this.avg = (double)(korSco+matSco+engSco)/3;
	}
	
	
	
	
}
