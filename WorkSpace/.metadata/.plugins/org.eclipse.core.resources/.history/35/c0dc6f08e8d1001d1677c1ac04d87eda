
public class Ex04_Oper {
	public static void main(String[] args) {
		//사칙연산 + 나머지
		int value1 =5, value2 =2;
//		int sum= value1+value2;
//		System.out.println(sum);
		System.out.println("value1 + value2 = " + (value1+value2));
		System.out.println("value1 - value2 = " + (value1-value2));
		//ㄴString - int가 불가능하기 때문에 소괄호로 우선순위
		System.out.println("value1 * value2 = " + (value1*value2));
		System.out.println("value1 / value2 = " + (value1/value2));
	}
}
