package apck08_casting;

public class Ex03_Casting {
	public static void main(String[] args) {
		String str1 = "123"+ 456;
		int i1 = Integer.parseInt(str1)+123456;
		System.out.println(i1);
		String str2 = 123 + "456";
		double d2 = Double.parseDouble(str2)+123456;
		System.out.println(d2);
		String str3 = "" + 123456;
		double d3 = Double.parseDouble(str3);
		System.out.println(str3);
	}

}
