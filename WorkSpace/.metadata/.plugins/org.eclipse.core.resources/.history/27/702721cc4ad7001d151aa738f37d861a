package pack02_array;

public class Ex02_Array {
public static void main(String[] args) {
	int[] scoreArray = new int[10];
	for(int i=0;i<10;i++) {
		scoreArray[i]=(i+1)*10;
		System.out.println("scoreArray["+i+"] : "+scoreArray[i]);
	}
	int j=0;
	while(j<10) {
		scoreArray[j]=(j+1)*10;
		System.out.println("scoreArray["+j+"] : "+scoreArray[j]);
		j++;
	}
	
	
	
	
	
	
	
}
}
