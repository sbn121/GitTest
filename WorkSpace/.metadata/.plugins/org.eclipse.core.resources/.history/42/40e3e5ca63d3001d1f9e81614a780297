package pack03_for;

public class Ex04_ForFor {
	public static void main(String[] args) {
		//제어문은 무한하게 중첩이 가능하다(순서x).
//		for(int i = 1;i<=10;i++) {
//			System.out.println("i "+i);
//		for(int j = 1; j<=10; j++) {
//			System.out.println("j "+j);
//			}
//		}
		
		for(int i = 2;i<10;i++) {
			System.out.println();
			for(int j = 1; j<10; j++) {
				System.out.print(i+"x"+j+"="+i*j+" ");
				}
			System.out.println();
				
			}
		int num = 0;
		for(int i1 = 1; i1<=5; i1++) {
			int sum = num+i1;
			System.out.println(num+"+"+i1);
		}
		
		
		
		
		
		}
	}

