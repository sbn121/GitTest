package pack03_for;

public class Ex04_ForFor {
	public static void main(String[] args) {
		//제어문은 무한하게 중첩이 가능하다(순서x).
//		for(int i = 1;i<=10;i++) {
//			System.out.println("i "+i);
//		for(int j = 1; j<=10; j++) {
//			System.out.println("j "+j);
//			}
//		}
		
		for(int i = 2;i<10;i++) {
			for(int j = 1; j<10; j++) {
				
				if(j!=9) {
					System.out.print(i+"x"+j+"="+i*j+", ");
				}else {
					System.out.println(i+"x"+j+"="+i*j);
				}
				}
				
			}
		}
	}

