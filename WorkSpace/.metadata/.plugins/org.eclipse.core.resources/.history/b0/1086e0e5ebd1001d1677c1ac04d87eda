
public class EX07_Oper {
	public static void main(String[] args) {
		//=(대입, 값 할당)
		// 비교 연산자 (==, <, >, <=, >=)
		// 초과, 미만 : 0<x<4 : 123
		// 이상, 이하 : 0<x<=4 : 1234
		int num = 10;
		String result = num ==10 ? "맞다" : "틀리다" ;
		//=과 ? 사이 내용이 맞으면 왼쪽 틀리면 오른쪽 출력
		int resultInt = num ==1 ? 1 : 2;
		System.out.println(result);
		System.out.println(resultInt);
	}
}
