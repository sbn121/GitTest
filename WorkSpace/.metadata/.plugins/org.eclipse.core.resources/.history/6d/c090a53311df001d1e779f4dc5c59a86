package pack06_japangi;

public class JapangiDTO {
	private int num, cost, count;
	private String name;
	
	
	public JapangiDTO(int num, String name, int cost, int count) {
		super();
		this.num = num;
		this.name = name;
		this.cost = cost;
		this.count = count;
	}
	
	
	
	public int getNum() {
		return num;
	}
	public void setNum(int num) {
		this.num = num;
	}
	public int getCost() {
		return cost;
	}
	public void setCost(int cost) {
		this.cost = cost;
	}
	public int getCount() {
		return count;
	}
	public void setCount(int count) {
		this.count = count;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
	
}
