package pack07.last.jdbc;

public class BoardMain {
	//main -> html, jsp, android app화면 등의 실제 사용자 눈에 보이는 메인화면
	public void displayMenu() {
		System.out.println("게시판 프로그램.");
		System.out.println("====메뉴====");
		System.out.print("게시글 조회 : S"); //select => Create
		System.out.print(" 게시글 추가 : I"); //insert => Read
		System.out.print(" 게시글 수정 : U"); //update
		System.out.print(" 게시글 삭제 : D"); //delete
		System.out.println(" 종료 : E");
		System.out.println("============");
	}
	
	
	public static void main(String[] args) {
		BoardMain bm = new BoardMain();
		bm.displayMenu();
	}
}
